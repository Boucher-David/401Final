{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension allows the user to change the background color of the current page.",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "VaultLogoLogout.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ]
}
